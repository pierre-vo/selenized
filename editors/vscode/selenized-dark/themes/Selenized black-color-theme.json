{
	"name": "Selenized Dark",
	"tokenColors": [
		{
			"settings": {
				"background": "#181818",
				"foreground": "#b9b9b9" // fg_0
			}
		},
		{
			"name": "Comment",
			"scope": "comment",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#777777" // dim_0
			}
		},
		{
			"name": "String",
			"scope": "string",
			"settings": {
				"foreground": "#3fc5b7" // cyan
			}
		},
		{
			"name": "Regexp",
			"scope": "string.regexp",
			"settings": {

				"foreground": "#efc541" // bright yellow
			}
		},
		{
			"name": "Constants (including numbers)",
			"scope": "constant",
			"settings": {
				// "foreground": "#dedede", // pink
			}
		},
		{
			"name": "Language constants (booleans)",
			"scope": "constant.language",
			"settings": {
				"foreground": "#eb6eb7"
			}
		},
		{
			"name": "Formatting placeholders, escaped characters",
			"scope": [
				"constant.other.placeholder",
				"constant.character.format",
				"constant.character.escape",
				"punctuation.section.embedded.begin",
				"punctuation.section.embedded.end"
			],
			"settings": {
				"foreground": "#368aeb", // blue
			}
		},
		{
			"name": "constant.other",
			"scope": "constant.other",
			"settings": {
				// "foreground": "#a580e2" // violet
			}
		},
		{
			"name": "Keyword control flow",
			"scope": "keyword.control",
			"settings": {
				"foreground": "#e67f43" // orange
			}
		},
		{
			"name": "Storage types",
			"scope": [
				"storage.type",
				"keyword.type",
			],
			"settings": {
				"foreground": "#b9b9b9" // fg_0
			}
		},
		{
			"name": "Storage modifier",
			"scope": [
				"storage.modifier"
			],
			"settings": {
				// "foreground": "#a580e2"
			}
		},
		{
			"name": "entity.name",
			"scope": "entity.name",
			"settings": {
				"foreground": "#dbb32d" // violet
			}
		},
		{
			"name": "Function name",
			"scope": "entity.name.function",
			"settings": {
				"foreground": "#368aeb" // blue
			}
		},

		{
			"name": "Variable",
			"scope": [
				"variable.language",
				"variable.other",
				"entity.name.variable"
			],
			"settings": {
				"foreground": "#b9b9b9"
			}
		},
		{
			"name": "Function parameter",
			"scope": [
				"variable.parameter"
			],
			"settings": {
				// "foreground": "#dedede",
			}
		},
		// {
		// 	"name": "Variable start",
		// 	"scope": "punctuation.definition.variable",
		// 	"settings": {
		// 		"foreground": "#70b433"
		// 	}
		// },

		{
			"name": "Tag attribute (HTML)",
			"scope": "entity.other.attribute-name",
			"settings": {
				"foreground": "#368aeb" // blue
			}
		},
		{
			"name": "Library/builtin function",
			"scope": "support.function",
			"settings": {
				"foreground": "#368aeb" // blue
			}
		},
		{
			"name": "Library class/type",
			"scope": [
				"support.type",
				"support.class"
			],
			"settings": {
				"foreground": "#70b433" // green
			}
		},
		{
			"name": "Continuation",
			"scope": "punctuation.separator.continuation",
			"settings": {
				"foreground": "#ed4a46" // red
			}
		},
		{
			"name": "Invalid",
			"scope": "invalid",
			"settings": {
				"foreground": "#ff5e56" // bright red
			}
		}
	],
