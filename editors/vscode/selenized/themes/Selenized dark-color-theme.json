{
  "name": "Selenized Dark",
  "tokenColors": [
    {
      "settings": {
        "background": "#103c48",
        "foreground": "#adbcbc" // fg_0
      }
    },
    {
      "name": "Comment",
      "scope": "comment",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#72898f" // dim_0
      }
    },
    {
      "name": "String",
      "scope": "string",
      "settings": {
        "foreground": "#41c7b9" // cyan
      }
    },
    {
      "name": "Regexp",
      "scope": "string.regexp",
      "settings": {
        "foreground": "#ebc13d" // bright yellow
      }
    },
    {
      "name": "Constants (including numbers)",
      "scope": "constant",
      "settings": {
        // "foreground": "#cad8d9", // pink
      }
    },
    {
      "name": "Language constants (booleans)",
      "scope": "constant.language",
      "settings": {
        "foreground": "#f275be"
      }
    },
    {
      "name": "Formatting placeholders, escaped characters",
      "scope": [
        "constant.other.placeholder",
        "constant.character.format",
        "constant.character.escape",
        "punctuation.section.embedded.begin",
        "punctuation.section.embedded.end"
      ],
      "settings": {
        "foreground": "#4695f7" // blue
      }
    },
    {
      "name": "constant.other",
      "scope": "constant.other",
      "settings": {
        // "foreground": "#af88eb" // violet
      }
    },
    {
      "name": "Keyword control flow",
      "scope": "keyword.control",
      "settings": {
        "foreground": "#ed8649" // orange
      }
    },
    {
      "name": "Storage types",
      "scope": ["storage.type", "keyword.type", "storage.modifier"],
      "settings": {
        "foreground": "#af88eb" // violet
      }
    },
    {
      "name": "entity.name",
      "scope": "entity.name",
      "settings": {
        "foreground": "#dbb32d" // yellow
      }
    },
    {
      "name": "Function name",
      "scope": "entity.name.function",
      "settings": {
        "foreground": "#4695f7" // blue
      }
    },

    {
      "name": "Variable",
      "scope": ["variable.language", "variable.other", "entity.name.variable"],
      "settings": {
        "foreground": "#adbcbc"
      }
    },
    {
      "name": "Function parameter",
      "scope": ["variable.parameter"],
      "settings": {
        // "foreground": "#cad8d9",
      }
    },
    // {
    // 	"name": "Variable start",
    // 	"scope": "punctuation.definition.variable",
    // 	"settings": {
    // 		"foreground": "#75b938"
    // 	}
    // },

    {
      "name": "Tag attribute (HTML)",
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#4695f7" // blue
      }
    },
    {
      "name": "Library/builtin function",
      "scope": "support.function",
      "settings": {
        "foreground": "#4695f7" // blue
      }
    },
    {
      "name": "Library class/type",
      "scope": ["support.type", "support.class"],
      "settings": {
        "foreground": "#75b938" // green
      }
    },
    {
      "name": "Continuation",
      "scope": "punctuation.separator.continuation",
      "settings": {
        "foreground": "#fa5750" // red
      }
    },
    {
      "name": "Invalid",
      "scope": "invalid",
      "settings": {
        "foreground": "#ff665c" // bright red
      }
    },
    {
      "name": "diff: header",
      "scope": ["meta.diff", "meta.diff.header"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#4695f7" // blue
      }
    },
    {
      "name": "diff: deleted",
      "scope": "markup.deleted",
      "settings": {
        "foreground": "#fa5750" // red
      }
    },
    {
      "name": "diff: changed",
      "scope": "markup.changed",
      "settings": {
        "foreground": "#dbb32d" // yellow
      }
    },
    {
      "name": "diff: inserted",
      "scope": ["markup.inserted", "markup.quote"],
      "settings": {
        "foreground": "#75b938" // green
      }
    },
    {
      "name": "Markup Lists",
      "scope": "markup.list",
      "settings": {
        "foreground": "#ed8649" // orange
      }
    },
    {
      "name": "Markup Styling",
      "scope": ["markup.bold", "markup.italic"],
      "settings": {
        "foreground": "#f275be" // magenta
      }
    },
    {
      "name": "Markup: Strong",
      "scope": "markup.bold",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup: Emphasis",
      "scope": "markup.italic",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": "markup.strikethrough",
      "settings": {
        "fontStyle": "strikethrough"
      }
    },
    {
      "name": "Markup Inline",
      "scope": "markup.inline.raw",
      "settings": {
        "fontStyle": "",
        "foreground": "#41c7b9" // cyan
      }
    },
    {
      "name": "Markup Headings",
      "scope": ["markup.heading"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#4695f7" // blue
      }
    },
    {
      "name": "Markup Setext Header",
      "scope": "markup.heading.setext",
      "settings": {
        "fontStyle": "",
        "foreground": "#4695f7" // blue
      }
    }
  ],
