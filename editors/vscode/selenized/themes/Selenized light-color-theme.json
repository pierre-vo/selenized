{
	"name": "Selenized Dark",
	"tokenColors": [
		{
			"settings": {
				"background": "#fbf3db",
				"foreground": "#53676d" // fg_0
			}
		},
		{
			"name": "Comment",
			"scope": "comment",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#909995" // dim_0
			}
		},
		{
			"name": "String",
			"scope": "string",
			"settings": {
				"foreground": "#009c8f" // cyan
			}
		},
		{
			"name": "Regexp",
			"scope": "string.regexp",
			"settings": {

				"foreground": "#a78300" // bright yellow
			}
		},
		{
			"name": "Constants (including numbers)",
			"scope": "constant",
			"settings": {
				// "foreground": "#3a4d53", // pink
			}
		},
		{
			"name": "Language constants (booleans)",
			"scope": "constant.language",
			"settings": {
				"foreground": "#ca4898"
			}
		},
		{
			"name": "Formatting placeholders, escaped characters",
			"scope": [
				"constant.other.placeholder",
				"constant.character.format",
				"constant.character.escape",
				"punctuation.section.embedded.begin",
				"punctuation.section.embedded.end"
			],
			"settings": {
				"foreground": "#0072d4", // blue
			}
		},
		{
			"name": "constant.other",
			"scope": "constant.other",
			"settings": {
				// "foreground": "#8762c6" // violet
			}
		},
		{
			"name": "Keyword control flow",
			"scope": "keyword.control",
			"settings": {
				"foreground": "#c25d1e" // orange
			}
		},
		{
			"name": "Storage types",
			"scope": [
				"storage.type",
				"keyword.type",
				"storage.modifier"
			],
			"settings": {
				"foreground": "#8762c6" // violet
			}
		},
		{
			"name": "entity.name",
			"scope": "entity.name",
			"settings": {
				"foreground": "#ad8900" // yellow
			}
		},
		{
			"name": "Function name",
			"scope": "entity.name.function",
			"settings": {
				"foreground": "#0072d4" // blue
			}
		},

		{
			"name": "Variable",
			"scope": [
				"variable.language",
				"variable.other",
				"entity.name.variable"
			],
			"settings": {
				"foreground": "#53676d"
			}
		},
		{
			"name": "Function parameter",
			"scope": [
				"variable.parameter"
			],
			"settings": {
				// "foreground": "#3a4d53",
			}
		},
		// {
		// 	"name": "Variable start",
		// 	"scope": "punctuation.definition.variable",
		// 	"settings": {
		// 		"foreground": "#489100"
		// 	}
		// },

		{
			"name": "Tag attribute (HTML)",
			"scope": "entity.other.attribute-name",
			"settings": {
				"foreground": "#0072d4" // blue
			}
		},
		{
			"name": "Library/builtin function",
			"scope": "support.function",
			"settings": {
				"foreground": "#0072d4" // blue
			}
		},
		{
			"name": "Library class/type",
			"scope": [
				"support.type",
				"support.class"
			],
			"settings": {
				"foreground": "#489100" // green
			}
		},
		{
			"name": "Continuation",
			"scope": "punctuation.separator.continuation",
			"settings": {
				"foreground": "#d2212d" // red
			}
		},
		{
			"name": "Invalid",
			"scope": "invalid",
			"settings": {
				"foreground": "#cc1729" // bright red
			}
		},
		{
			"name": "diff: header",
			"scope": [
				"meta.diff",
				"meta.diff.header"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#0072d4" // blue
			}
		},
		{
			"name": "diff: deleted",
			"scope": "markup.deleted",
			"settings": {
				"foreground": "#d2212d" // red
			}
		},
		{
			"name": "diff: changed",
			"scope": "markup.changed",
			"settings": {
				"foreground": "#ad8900" // yellow
			}
		},
		{
			"name": "diff: inserted",
			"scope": [
				"markup.inserted",
				"markup.quote"
			],
			"settings": {
				"foreground": "#489100" // green
			}
		},
		{
			"name": "Markup Lists",
			"scope": "markup.list",
			"settings": {
				"foreground": "#c25d1e" // orange
			}
		},
		{
			"name": "Markup Styling",
			"scope": [
				"markup.bold",
				"markup.italic"
			],
			"settings": {
				"foreground": "#ca4898" // magenta
			}
		},
		{
			"name": "Markup: Strong",
			"scope": "markup.bold",
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"name": "Markup: Emphasis",
			"scope": "markup.italic",
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"scope": "markup.strikethrough",
			"settings": {
				"fontStyle": "strikethrough"
			}
		},
		{
			"name": "Markup Inline",
			"scope": "markup.inline.raw",
			"settings": {
				"fontStyle": "",
				"foreground": "#009c8f" // cyan
			}
		},
		{
			"name": "Markup Headings",
			"scope": [
				"markup.heading",
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#0072d4" // blue
			}
		},
		{
			"name": "Markup Setext Header",
			"scope": "markup.heading.setext",
			"settings": {
				"fontStyle": "",
				"foreground": "#0072d4" // blue
			}
		}
	],
