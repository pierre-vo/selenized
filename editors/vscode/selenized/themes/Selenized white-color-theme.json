{
	"name": "Selenized Dark",
	"tokenColors": [
		{
			"settings": {
				"background": "#ffffff",
				"foreground": "#474747" // fg_0
			}
		},
		{
			"name": "Comment",
			"scope": "comment",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#878787" // dim_0
			}
		},
		{
			"name": "String",
			"scope": "string",
			"settings": {
				"foreground": "#00ad9c" // cyan
			}
		},
		{
			"name": "Regexp",
			"scope": "string.regexp",
			"settings": {

				"foreground": "#af8500" // bright yellow
			}
		},
		{
			"name": "Constants (including numbers)",
			"scope": "constant",
			"settings": {
				// "foreground": "#282828", // pink
			}
		},
		{
			"name": "Language constants (booleans)",
			"scope": "constant.language",
			"settings": {
				"foreground": "#dd0f9d"
			}
		},
		{
			"name": "Formatting placeholders, escaped characters",
			"scope": [
				"constant.other.placeholder",
				"constant.character.format",
				"constant.character.escape",
				"punctuation.section.embedded.begin",
				"punctuation.section.embedded.end"
			],
			"settings": {
				"foreground": "#0064e4", // blue
			}
		},
		{
			"name": "constant.other",
			"scope": "constant.other",
			"settings": {
				// "foreground": "#7f51d6" // violet
			}
		},
		{
			"name": "Keyword control flow",
			"scope": "keyword.control",
			"settings": {
				"foreground": "#d04a00" // orange
			}
		},
		{
			"name": "Storage types",
			"scope": [
				"storage.type",
				"keyword.type",
				"storage.modifier"
			],
			"settings": {
				"foreground": "#7f51d6" // violet
			}
		},
		{
			"name": "entity.name",
			"scope": "entity.name",
			"settings": {
				"foreground": "#c49700" // yellow
			}
		},
		{
			"name": "Function name",
			"scope": "entity.name.function",
			"settings": {
				"foreground": "#0064e4" // blue
			}
		},

		{
			"name": "Variable",
			"scope": [
				"variable.language",
				"variable.other",
				"entity.name.variable"
			],
			"settings": {
				"foreground": "#474747"
			}
		},
		{
			"name": "Function parameter",
			"scope": [
				"variable.parameter"
			],
			"settings": {
				// "foreground": "#282828",
			}
		},
		// {
		// 	"name": "Variable start",
		// 	"scope": "punctuation.definition.variable",
		// 	"settings": {
		// 		"foreground": "#1d9700"
		// 	}
		// },

		{
			"name": "Tag attribute (HTML)",
			"scope": "entity.other.attribute-name",
			"settings": {
				"foreground": "#0064e4" // blue
			}
		},
		{
			"name": "Library/builtin function",
			"scope": "support.function",
			"settings": {
				"foreground": "#0064e4" // blue
			}
		},
		{
			"name": "Library class/type",
			"scope": [
				"support.type",
				"support.class"
			],
			"settings": {
				"foreground": "#1d9700" // green
			}
		},
		{
			"name": "Continuation",
			"scope": "punctuation.separator.continuation",
			"settings": {
				"foreground": "#d6000c" // red
			}
		},
		{
			"name": "Invalid",
			"scope": "invalid",
			"settings": {
				"foreground": "#bf0000" // bright red
			}
		},
		{
			"name": "diff: header",
			"scope": [
				"meta.diff",
				"meta.diff.header"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#0064e4" // blue
			}
		},
		{
			"name": "diff: deleted",
			"scope": "markup.deleted",
			"settings": {
				"foreground": "#d6000c" // red
			}
		},
		{
			"name": "diff: changed",
			"scope": "markup.changed",
			"settings": {
				"foreground": "#c49700" // yellow
			}
		},
		{
			"name": "diff: inserted",
			"scope": [
				"markup.inserted",
				"markup.quote"
			],
			"settings": {
				"foreground": "#1d9700" // green
			}
		},
		{
			"name": "Markup Lists",
			"scope": "markup.list",
			"settings": {
				"foreground": "#d04a00" // orange
			}
		},
		{
			"name": "Markup Styling",
			"scope": [
				"markup.bold",
				"markup.italic"
			],
			"settings": {
				"foreground": "#dd0f9d" // magenta
			}
		},
		{
			"name": "Markup: Strong",
			"scope": "markup.bold",
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"name": "Markup: Emphasis",
			"scope": "markup.italic",
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"scope": "markup.strikethrough",
			"settings": {
				"fontStyle": "strikethrough"
			}
		},
		{
			"name": "Markup Inline",
			"scope": "markup.inline.raw",
			"settings": {
				"fontStyle": "",
				"foreground": "#00ad9c" // cyan
			}
		},
		{
			"name": "Markup Headings",
			"scope": [
				"markup.heading",
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#0064e4" // blue
			}
		},
		{
			"name": "Markup Setext Header",
			"scope": "markup.heading.setext",
			"settings": {
				"fontStyle": "",
				"foreground": "#0064e4" // blue
			}
		}
	],
